{
  "name": "argocd-diff-action",
  "version": "0.4.1",
  "private": true,
  "description": "ArgoCD Diff GitHub Action",
  "main": "src/main.ts",
  "pre-commit": [
    "precommit-msg",
    "format",
    "lint"
  ],
  "type": "module",
  "scripts": {
    "build": "ncc build",
    "format-check": "prettier --check **.ts",
    "format": "prettier --write **.ts",
    "lint": "eslint src/*.ts src/**/*.ts",
    "precommit-msg": "echo 'Running pre-commit hooks..' && exit 0",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argocd-diff-action/argocd-diff-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "argocd"
  ],
  "author": "argocd-diff-action",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@fetch-mock/jest": "^0.2.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/release-notes-generator": "^14.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "@types/sinon": "^17.0.3",
    "@vercel/ncc": "^0.38.3",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "fetch-mock": "^12.2.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8",
    "semantic-release": "^24.2.0",
    "sinon": "^19.0.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
